{
    "name": "__MSG_appName__",
    "short_name": "__MSG_appShortName__",
    "description": "__MSG_appDescription__",
    "version": "1.2.14",
    "manifest_version": 2,
    "default_locale": "en",
    "offline_enabled": false,
    "permissions": [
        "notifications",
        "storage",
        "unlimitedStorage",
        "webview",
        "*://fimfiction.net/",
        "*://*.fimfiction.net/",
        "http://www.fimfiction.net/",
        "https://www.fimfiction.net/",
        "*://fimfiction.net/*",
        "*://*.fimfiction.net/*"
    ],
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "app": {
        "background": {
            "scripts": [
                "init.js",
                "chromereload.js"
            ],
            "persistent": false
        }
    }
}
